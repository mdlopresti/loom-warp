{
  "name": "@loom/warp",
  "version": "1.2.0",
  "description": "Warp - The messaging backbone for Loom multi-agent infrastructure",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "warp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "loom",
    "warp",
    "mcp",
    "nats",
    "jetstream",
    "agent",
    "claude",
    "ai",
    "messaging",
    "multi-agent",
    "weaving"
  ],
  "author": "Michael LoPresti <mike@lopresti.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdlopresti/loom-warp.git"
  },
  "homepage": "https://github.com/mdlopresti/loom-warp#readme",
  "bugs": {
    "url": "https://github.com/mdlopresti/loom-warp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "nats": "^2.28.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "schemas"
  ]
}
